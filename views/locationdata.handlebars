<div class="container">
    <div class="jumbotron">
        <center>
            <h3>{{title}}</h3>
        </center>
    </div>

    <table id="locationd" class="table table-dart">
        <thead>
            <tr class="text-center">
                <th scope="col">Index</th>
                <th scope="col">Latitude</th>
                <th scope="col">Longitude</th>
                <th scope="col">Device ID</th>
                <th scope="col">Time Stamp</th>
            </tr>
        </thead>
        <tbody>
            {{#each locations}}
                <tr>
                    <td class="text-center">{{@index}}</td>
                    <td class="text-center">{{latitude}}</td>
                    <td class="text-center">{{longitude}}</td>
                    <td class="text-center">{{device.deviceId}}</td>
                    <td class="text-center">{{createdAt}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>

</div>
<script>
$(document).ready(function() {
    $('#locationd').DataTable( {
        dom: 'Bfrtip',
        buttons: [
            'copyHtml5',
            'excelHtml5',
            'csvHtml5',
            'pdfHtml5'
        ]
    });
});
</script>